public class Tac extends Thread {
	//Propiedades --------------------------------
	private int hilo;

	//Constructor --------------------------------
	public Tac (int hilo){
		this.hilo=hilo;
	}//fin constructor
	
	//MÃ©todo Run -----------------------------------
	public void run(){

		while (true){

		   try {		
       			  sleep(1000);
			 } catch (InterruptedException e) {
                    }


			System.out.println ("TAC");

		}
	}//fin run
	

} //fin clase
