
public class Persona extends Thread{

	private Cuenta cuenta;
	private int n;

	public Persona (Cuenta c, int n){
		cuenta=c;
		this.n=n;
	}
	
	public void run(){

				cuenta.meter(i); //pone el n√∫mero
				System.out.println("Ingresa: "+i);
	 			try {
					sleep(100);
				}catch (InterruptedException e){}
			
		}
	
	
}
