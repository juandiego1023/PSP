
public class Persona extends Thread{

	private Cuenta cuenta;
	private int n;

	public Persona (Cuenta c, int n){
		cuenta=c;
		this.n=n;
	}
	
	public void run(){

			for (int i=0;i<5;i++){
				cuenta.meter(i); //pone el nÃºmero
				System.out.println("Ingresa: "+i);
	 			try {
					sleep(100);
				}catch (InterruptedException e){}
			}
		}
	
	
}
